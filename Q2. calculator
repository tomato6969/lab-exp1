#include <iostream>
using namespace std;
int main(){
    
    int num1 , num2 , sum=0;
    cout <<"enter the first number:" << num1;
    cin>>num1;
    cout<<"enter the second number:" << num2;
    cin>>num2;
    sum=num1+num2;
    cout<<"sum of given numbers ="<<sum;
    
    return 0;
}

OUTPUT :
enter the first number: 3
enter the second number: 2
sum of given numbers = 5
